{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.js",
      "use": "@vercel/node",
      "config": {
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}