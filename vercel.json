{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "src/api/**/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}